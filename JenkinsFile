pipeline {
    agent any

    tools {
         maven "mymaven"
        jdk "javajdk"
        git "mygit"
    }

    stages {
        stage('Checkout') {
            steps {
                git branch : 'main',
                url: 'https://github.com/aniz-star/Nov27jen.git'
            }
        }
stage('Clean Package') {
            steps {
                echo " Cleaning Package"
                sh "mvn  clean package"
            }
        }

            stage ('Nested Testing' ) { 
                parallel {
stages {                  
stage('Performance Testing') {
            steps { 
                echo "Stage3 - Performance Testing"
            }
              }
             stage('Code Quality Check Test') {
            steps { 
                echo "Stage3 -Code Quality Check Test"
            }
        }  
}
stage ('Sequntial Testing') {
stages {
stage('Acceptance Testing') { 
                        steps {
                echo "Stage3- Acceptance Testing"
                    }
           }
stage('Regression Testing') { 
                        steps {
                echo "Stage3 - Regression Testing"
                    }
           }
                }
}
}
}
}
}

